                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
    SOD.SalesOrderID AS  SalesOrder_ID ,
    SOD.ProductID AS Product_ID,
	CONCAT(
        CAST(YEAR(SOH.OrderDate) AS varchar(4)),
        CAST(RIGHT('0' + RTRIM(MONTH(SOH.OrderDate)), 2) AS varchar(2)),
        CAST(RIGHT('0' + RTRIM(DAY(SOH.OrderDate)), 2) AS varchar(2))
    ) AS 'Codigo_Tiempo',
	SOH.CustomerID,
	SOD.OrderQty AS Order_Qty ,
	SOD.UnitPrice AS Unit_Price,
	P.StandardCost AS Standard_Cost,
	SOD.LineTotal AS Line_Total,
	SOD.OrderQty*P.StandardCost AS Total_Cost

	
FROM 
    [Sales].[SalesOrderDetail] AS SOD
INNER JOIN 
    [Sales].[SalesOrderHeader] AS SOH ON SOD.SalesOrderID = SOH.SalesOrderID
INNER JOIN 
    [Production].[Product] AS P ON P.ProductID = SOD.ProductID</property>
